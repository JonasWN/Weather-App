  </div>
  <!-- Wrapper-Ends -->
  <script src="assets/js/current-weather.js" defer></script>
  <script src="assets/js/burgerMenu.js" defer></script>
  <script src="assets/js/date.js" defer></script>
  <script src="assets/js/forecast.js" defer></script>
</body>

</html>